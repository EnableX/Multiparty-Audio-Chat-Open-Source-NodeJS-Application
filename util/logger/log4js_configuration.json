{
  "appenders": [
    {
      "type": "file",
      "filename": "./../logs/app.log",
      "maxLogSize": 1048576,
      "backups": 5,
      "layout": {
        "type": "pattern",
        "pattern": "%d{ddMMyyhhmmss}::%p::%c::%m",
        "replaceConsole": true
      }
    }
  ],
  "levels": {
    "AppApi": "INFO",
    "Authorization":"INFO",
    "Server":"INFO",
    "ErrorHandling":"INFO"
  }
}